{
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  // "window.titleBarStyle": "native",
  "git.autofetch": true,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "workbench.startupEditor": "none",
  "workbench.activityBar.location": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "css.lint.unknownAtRules": "ignore",
  "editor.largeFileOptimizations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.lineNumbers": "off",
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.guides.indentation": false,
  "workbench.iconTheme": null,
  "editor.scrollBeyondLastLine": false,
  // "git.blame.editorDecoration.enabled": true,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${profileName}${separator}${activeRepositoryBranchName}${separator}${dirty}",
  "workbench.statusBar.visible": false,
  "scm.diffDecorations": "none",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "editor.scrollBeyondLastColumn": 0,
  // "editor.formatOnPaste": true,
  "diffEditor.codeLens": true,
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,
  "editor.renderLineHighlight": "none",
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "explicit",
  "workbench.editor.showTabs": "none",
  "editor.stickyScroll.enabled": false,
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": "off",
  "editor.matchBrackets": "never",
  "editor.guides.bracketPairs": true,
  "editor.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "editor.lineHeight": 1.1,
  "explorer.decorations.badges": false,
  "workbench.colorCustomizations": {
    "[Default Light Modern]": {
      "sideBar.background": "#ffffff",
      "sideBar.border": "#ffffff",
      "widget.shadow": "#0000",
      "editorWidget.border": "#ffffff",
      "editorWidget.background": "#ffffff",
      "scrollbarSlider.activeBackground": "#0000",
      "scrollbarSlider.background": "#0000",
      "scrollbarSlider.hoverBackground": "#0000",
      "list.hoverBackground": "#0000",
      "list.activeSelectionBackground": "#eeeeee",
      "list.inactiveSelectionBackground": "#eeeeee",
      "panel.border": "#0000",
      "panel.background": "#ffffff",
      "focusBorder": "#0000",
      "editorGroup.border": "#0000",
      "editor.wordHighlightBackground": "#0000",
      "editor.wordHighlightBorder": "#0080ff",
      "editor.wordHighlightStrongBackground": "#0000",
      "editor.wordHighlightStrongBorder": "#0080ff",
      "editorCursor.foreground": "#007bff",
      "scrollbar.shadow": "#0000"
    },
    "[Default Dark Modern]": {
      "sideBar.background": "#1f1f1f",
      "sideBar.border": "#1f1f1f",
      "widget.shadow": "#0000",
      "editorWidget.border": "#1f1f1f",
      "editorWidget.background": "#1f1f1f",
      "scrollbarSlider.activeBackground": "#0000",
      "scrollbarSlider.background": "#0000",
      "scrollbarSlider.hoverBackground": "#0000",
      "list.hoverBackground": "#0000",
      "list.activeSelectionBackground": "#0000",
      "list.inactiveSelectionBackground": "#0000",
      "focusBorder": "#0000",
      "panel.border": "#0000",
      "panel.background": "#1f1f1f",
      "editorGroup.border": "#0000",
      "editor.wordHighlightBackground": "#0000",
      "editor.wordHighlightBorder": "#0080ff",
      "editor.wordHighlightStrongBackground": "#0000",
      "editor.wordHighlightStrongBorder": "#0080ff",
      "editorCursor.foreground": "#007bff",
      "scrollbar.shadow": "#0000"
    }
  },
  "telemetry.telemetryLevel": "off",
  "workbench.tree.enableStickyScroll": false,
  "window.zoomLevel": -1,
  "editor.lineDecorationsWidth": 0,
  "editor.lineNumbersMinChars": 0,
  "go.toolsManagement.autoUpdate": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "chat.agent.enabled": false,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "breadcrumbs.enabled": false,
  "terminal.integrated.defaultLocation": "editor"
}
